package LabOne;
import LabOne.Student;
public class StudentTest {
    //Custom objects
	private static Student s1 = new Student(), s2 = new Student(), s3 = new Student();
    
	public static void main(String[] args) {
		// TODO Auto-generated method stub
        //set names
		s1.setName("Bob");
		//will cause an error
		s2.setName(null);
		s3.setName("Tom");
		
		//sets the tests
		//will cause an error
		s1.setTest(1, 101);
		s2.setTest(1, 3);
		s3.setTest(1, 99);
		s3.setTest(2, 98);
		String vs1 = s1.validateData(),
			   vs2 = s2.validateData(),
			   vs3 = s3.validateData();
		if(vs1 == null){System.out.println("Student 1 is valid");}
		System.out.println(vs1);
		if(vs2 == null){System.out.println("Student 2 is valid");}
	    System.out.println(vs2);
		if(vs3 == null){System.out.println("Student 3 is valid");}
		System.out.println(vs3);
		
	}

}
