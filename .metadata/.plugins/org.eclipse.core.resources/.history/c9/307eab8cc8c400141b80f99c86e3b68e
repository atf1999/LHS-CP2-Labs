
public class Pi {
		public static void main(String[] args){
			double sum = 0;
			for(int i = 0; i < 100000; ++i){
				if(i%2 == 0){
                    sum += -1 / ( 2 * i - 1);
                    System.out.println(sum);
				}
				else
                    sum += 1 / (2 * i - 1);
			}
			System.out.println(sum);
		}
}
